{
  "name": "graphql-with-microorm",
  "version": "1.0.0",
  "description": "This is a simple graphql application that I made by following one of Ben Awad's tutorials.",
  "main": "src/index.js",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "dev2": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts"
  },
  "keywords": ["graphql", "microorm", "typescript"],
  "author": {
    "name": "Dalekvim",
    "email": "dalekvim@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dalekvim/graphql-with-microorm.git"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.3.4",
    "@mikro-orm/core": "^4.3.4",
    "@mikro-orm/migrations": "^4.3.4",
    "@mikro-orm/mongodb": "^4.3.4",
    "apollo-server-express": "^2.19.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "mongodb": "^3.6.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}